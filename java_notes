																			               age =7;
																				                      switch(age){
																						                     case 1:
																								                          System. out.println("You can crawl" );
																											                        break;
																														               case 2:
																															                            System. out.println("You can speak" );
																																		                          break;
																																					                 case 3:
																																							                      System. out.println("You can make trouble" );
																																									                            break;
																																												                   default:
																																														                        System. out.println("Don\'t know how old of you" );
																																																	                     
																																																			                   }
